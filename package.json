{
  "name": "webutvikling-api-design",
  "version": "1.0.0",
  "description": "",
  "main": "server/server.js",
  "type": "module",
  "scripts": {
    "start": "cd server && npm start",
    "build": "npm run build:client && npm run build:server",
    "build:server": "cd server && npm install",
    "build:client": "cd client && npm install --include=dev && npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "verify": "npm run test:coverage",
    "clean": "rimraf dist .parcel-cache",
    "format:check": "prettier --check \"**/*.{jsx,js}\" \"__test__/**/*.{jsx,js}\"",
    "prettier:write": "prettier --write \"**/*.{jsx,js}\" \"__test__/**/*.{jsx,js}\"",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "babel-jest": "^27.5.1",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessicafuung/webutvikling-api-design.git"
  },
  "bugs": {
    "url": "https://github.com/jessicafuung/webutvikling-api-design/issues"
  },
  "homepage": "https://github.com/jessicafuung/webutvikling-api-design#readme"
}
